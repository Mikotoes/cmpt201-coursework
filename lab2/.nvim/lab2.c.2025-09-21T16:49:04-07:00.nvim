#define _POSIX_C_SOURCE 200809L
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void) {
  char *lineptr = NULL;
  size_t len = 0;
  ssize_t input;
  printf("Enter programs to run.\n");
  input = getline(&lineptr, &len, stdin);
  if (input == -1) {
    free(lineptr);
    return 1;
  }
  printf("%s\n", lineptr);
  free(lineptr);
}
